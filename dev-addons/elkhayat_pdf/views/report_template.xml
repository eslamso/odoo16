<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data><!-- Register the custom invoice report -->
        <record id="paperformat_payslip_group" model="report.paperformat">
            <field name="name">European A4</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">33</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">25</field>
            <field name="dpi">90</field>
        </record>
        <report id="action_report_custom_invoice" string="elkhayat_pdf" model="account.move" report_type="qweb-pdf" name="elkhayat_pdf.elkhayat_pdf_report" file="elkhayat_pdf.elkhayat_pdf_report" paperformat="paperformat_payslip_group"/>
    </data>
</odoo>